program circles;

string colors[3] = ["Blue","Yellow","Green"];


mainF{
    int i = 1;
    int pos = 5;
    pointer_size(3);
    speed("fast");

    for(i:3){
        pointer_color(colors[i]);
        circle(i*40);
        up;
        pos = -1 * i;
        pos = pos * 10;
        position(0,pos);
        down;
    }

    exit;
}
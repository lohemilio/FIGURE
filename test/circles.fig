program circles;

string colors[10] = ["Thistle","Medium Purple","Purple","Blue Violet","Dark Violet","Dark Orchid","Medium Orchid","Orchid","Plum","Violet"];


mainF{
    int i = 1;
    int pos;
    pointer_size(3);
    speed("fast");

    for(i:10){
        pointer_color(colors[i]);
        circle(i*40);
        up;
        pos = -1 * i;
        pos = pos * 10;
        position(0,pos);
        down;
    }

    exit;
}